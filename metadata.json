{
  "name": "sharpie-op_data",
  "version": "0.2.0",
  "author": "Charlie Sharpsteen",
  "summary": "Bolt inventory plugin for getting data from 1password",
  "license": "Apache-2.0",
  "source": "https://github.com/Sharpie/bolt-op_data",
  "project_page": "https://github.com/Sharpie/bolt-op_data",
  "issues_url": "https://github.com/Sharpie/bolt-op_data/issues",
  "dependencies": [
    {
      "name": "puppetlabs-ruby_task_helper",
      "version_requirement": "1.x"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "8",
	"9"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "12",
	"13"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "24.04"
